    "@vercel/edge-config": "^1.2.0",
    "@vercel/og": "^0.6.2",
    "axios": "^1.7.2",
    "chakra-react-select": "^4.9.1",
    "eslint-config-next": "^14.2.5",
    "feed": "^4.2.2",
    "framer-motion": "^11.3.6",
    "gray-matter": "^4.0.3",
