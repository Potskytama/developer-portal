{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env", ".env.local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {},
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "devportal#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "test": {
      "cache": false
    },
    "start": {
      "dependsOn": ["^build"]
    }
  }
}
